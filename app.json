{
  "expo": {
    "name": "GiftsApp",
    "slug": "GiftsApp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logo.png",
    "splash": {
      "image": "./assets/logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#fffcf3"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.projectgiftapp.giftsapp",
      "buildNumber": "1.0.0"
    },
    "android": {
      "package": "com.projectgiftapp.giftsapp",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/logo.png",
        "backgroundColor": "#FFFCF3"
      }
    },
    "web": {
      "favicon": "./assets/logo.png"
    }
  }
}
